{
  "name": "flavor-switcher",
  "version": "1.0.0",
  "description": "Open-source CLI tool for managing white-label application resources",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "flavor-switcher": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "validate": "npm run build && node dist/index.js validate",
    "prepublishOnly": "npm run build && npm test",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "white-label",
    "brand",
    "multi-tenant",
    "resource-management",
    "cli",
    "configuration",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "fs-extra": "^11.1.0",
    "glob": "^8.0.3",
    "inquirer": "^8.2.5",
    "joi": "^17.7.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.0.0",
    "@types/inquirer": "^8.2.5",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/krzbbaranowski/flavor-switcher.git"
  },
  "bugs": {
    "url": "https://github.com/krzbbaranowski/flavor-switcher/issues"
  },
  "homepage": "https://github.com/krzbbaranowski/flavor-switcher#readme"
}